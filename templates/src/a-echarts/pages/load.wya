<template></template>
<script>
import * as echarts from 'echarts/dist/echarts.min';
import { core } from '../../framework/index.js';

core.page({
	onLoad(options) {
		this.options = options;
	},
	onReady() {
		try {
			getApp().$modules.echarts = echarts;
		
			let { url } = this.options;
			
			wx.nextTick(() => {
				wx.navigateAuto({
					url: decodeURIComponent(url),
					redirect: true
				});
			});
		} catch (e) {
			console.log(e);
		}
	}
});
</script>

<config>
{
	"navigationBarTitleText": "",
	"usingComponents": {}
}
</config>